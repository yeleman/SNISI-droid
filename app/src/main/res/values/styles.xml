<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>
    <style name="textViewLabel">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:linksClickable">false</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_margin">5dp</item>
        <!-- <item name="android:background">@color/abc_search_url_text_normal</item> -->
    </style>
    <style name="editText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ems">10</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:singleLine">false</item>
        <item name="android:editable">true</item>
        <!-- <item name="android:background">@color/abc_search_url_text_pressed</item> -->
    </style>
    <style name="editText050" parent="editText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">0.50</item>
    </style>
    <style name="editNumberText" parent="editText">
        <item name="android:inputType">number</item>
    </style>
    <style name="editNumberText050" parent="editText050">
        <item name="android:inputType">number</item>
    </style>
    <style name="editNumberDecimalText050" parent="editText050">
        <item name="android:inputType">numberDecimal</item>
    </style>
    <style name="summarytableRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="summaryTableLabel" parent="summaryTableLayout">
        <item name="android:gravity">center</item>
        <!--<item name="android:textStyle">bold</item>-->
    </style>
    <style name="sendButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
    </style>
    <style name="summaryTableLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shrinkColumns">*</item>
        <item name="android:stretchColumns">*</item>
        <item name="android:background">#fff</item>
        <item name="android:textColor">#000</item>
    </style>
</resources>
