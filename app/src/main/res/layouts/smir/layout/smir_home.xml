<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ui="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".SNISI" >
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView1"
        android:orientation="vertical">
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelEbola"
            android:text="@string/ebola"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutEbola"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedEbolaField"
                android:nextFocusRight="@+id/deathEbolaField"
                android:nextFocusDown="@+id/deathEbolaField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathEbolaField"
                android:nextFocusRight="@+id/confirmedAcuteFlaccidParalysisField"
                android:nextFocusDown="@+id/confirmedAcuteFlaccidParalysisField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelAcuteFlaccidParalysis"
            android:text="@string/acute_flaccid_paralysis"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutAcuteFlaccidParalysis"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedAcuteFlaccidParalysisField"
                android:nextFocusRight="@+id/deathAcuteFlaccidParalysisField"
                android:nextFocusDown="@+id/deathAcuteFlaccidParalysisField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathAcuteFlaccidParalysisField"
                android:nextFocusRight="@+id/confirmedInfluenzaAH1n1Field"
                android:nextFocusDown="@+id/confirmedInfluenzaAH1n1Field"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelInfluenzaAH1n1"
            android:text="@string/influenza_a_h1n1"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutInfluenzaAH1n1"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedInfluenzaAH1n1Field"
                android:nextFocusRight="@+id/deathInfluenzaAH1n1Field"
                android:nextFocusDown="@+id/deathInfluenzaAH1n1Field"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathInfluenzaAH1n1Field"
                android:nextFocusRight="@+id/confirmedCholeraField"
                android:nextFocusDown="@+id/confirmedCholeraField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelCholera"
            android:text="@string/cholera"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutCholera"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedCholeraField"
                android:nextFocusRight="@+id/deathCholeraField"
                android:nextFocusDown="@+id/deathCholeraField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathCholeraField"
                android:nextFocusRight="@+id/confirmedRedDiarrheaField"
                android:nextFocusDown="@+id/confirmedRedDiarrheaField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelRedDiarrhea"
            android:text="@string/red_diarrhea"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutRedDiarrhea"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedRedDiarrheaField"
                android:nextFocusRight="@+id/deathRedDiarrheaField"
                android:nextFocusDown="@+id/deathRedDiarrheaField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathRedDiarrheaField"
                android:nextFocusRight="@+id/confirmedRedDiarrheaField"
                android:nextFocusDown="@+id/confirmedRedDiarrheaField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelMeasles"
            android:text="@string/measles"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutMeasles"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedMeaslesField"
                android:nextFocusRight="@+id/deathMeaslesField"
                android:nextFocusDown="@+id/deathMeaslesField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathMeaslesField"
                android:nextFocusRight="@+id/confirmedYellowFeverField"
                android:nextFocusDown="@+id/confirmedYellowFeverField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelYellowFever"
            android:text="@string/yellow_fever"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutYellowFever"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedYellowFeverField"
                android:nextFocusRight="@+id/deathYellowFeverField"
                android:nextFocusDown="@+id/deathYellowFeverField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathYellowFeverField"
                android:nextFocusRight="@+id/confirmedNeonatalTetanusField"
                android:nextFocusDown="@+id/confirmedNeonatalTetanusField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelNeonatalTetanus"
            android:text="@string/neonatal_tetanus"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutNeonatalTetanus"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedNeonatalTetanusField"
                android:nextFocusRight="@+id/deathNeonatalTetanusField"
                android:nextFocusDown="@+id/deathNeonatalTetanusField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathNeonatalTetanusField"
                android:nextFocusRight="@+id/confirmedNeonatalTetanusField"
                android:nextFocusDown="@+id/confirmedNeonatalTetanusField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelMeningitis"
            android:text="@string/meningitis"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutMeningitis"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedMeningitisField"
                android:nextFocusRight="@+id/deathMeningitisField"
                android:nextFocusDown="@+id/deathMeningitisField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathMeningitisField"
                android:nextFocusRight="@+id/confirmedRabiesField"
                android:nextFocusDown="@+id/confirmedRabiesField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelRabies"
            android:text="@string/rabies"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutRabies"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedRabiesField"
                android:nextFocusRight="@+id/deathRabiesField"
                android:nextFocusDown="@+id/deathRabiesField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathRabiesField"
                android:nextFocusRight="@+id/confirmedacuteMeaslesDiarrheaField"
                android:nextFocusDown="@+id/confirmedacuteMeaslesDiarrheaField"
                android:imeOptions="actionNext"
                android:hint="@string/labeldeath"/>
        </LinearLayout>
        <TextView
            style="@style/textViewLabel"
            android:id="@+id/labelacuteMeaslesDiarrhea"
            android:text="@string/acute_measles_diarrhea"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutacuteMeaslesDiarrhea"
            android:orientation="horizontal">
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/confirmedacuteMeaslesDiarrheaField"
                android:nextFocusRight="@+id/deathacuteMeaslesDiarrheaField"
                android:nextFocusDown="@+id/deathacuteMeaslesDiarrheaField"
                android:imeOptions="actionNext"
                android:hint="@string/labelconfirmed" />
            <EditText
                style="@style/editNumberText050"
                android:id="@+id/deathacuteMeaslesDiarrheaField"
                android:imeOptions="actionDone"
                android:hint="@string/labeldeath"/>
        </LinearLayout>

        <TextView
            style="@style/textViewLabel"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Fin de la semaine (date du vendredi)."
            android:id="@+id/labelDate"/>

        <DatePicker
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/datePicker"
            android:layout_gravity="center_horizontal"
            android:layout_margin="10dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/sendButton"
            android:id="@+id/submitButton"
            android:layout_margin="10dp" />
    </LinearLayout>
</ScrollView>