<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TableLayout
        style="@style/summaryTableLayout">
        <TableRow
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/titleLabelUREN"
                android:textSize="18dp"
                android:text="URENAM + URENAS"
                android:layout_span="3"
                android:padding="5dp"
                android:background="#b0b0b0"
                android:textStyle="bold"/>
        </TableRow>
        <TableRow
            android:id="@+id/sexRow2"
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/totalMText"
                android:text="@string/nutrition_m"
                android:layout_weight="1"
                android:textStyle="bold"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/totalFText"
                android:text="@string/nutrition_f"
                android:layout_weight="1"
                android:background="#fff8fbff"
                android:textStyle="bold"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/totalsText"
                android:text="Total"
                android:textStyle="bold" />
        </TableRow>
        <!-- Total start month -->
        <TableRow
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:textSize="18dp"
                android:text="@string/nutrition_total_start"
                android:layout_span="3"
                android:background="#d3d3d3" />

        </TableRow>
        <TableRow
            android:id="@+id/tableRow1"
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/startTotalValueM"
                android:layout_weight="1"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/startTotalValueF"
                android:layout_weight="1"
                android:background="#fff1f4f8" />
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/startTotalsValue"
                android:layout_weight="1"/>
        </TableRow>
        <!-- Admmission -->
        <TableRow
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:textSize="18dp"
                android:text="@string/nutrition_admission"
                android:layout_span="3"
                android:background="#d3d3d3"/>
        </TableRow>
        <TableRow
            android:id="@+id/admissionTableRow"
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/admissionTotalValueM"
                android:layout_weight="1"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/admissionTotalValueF"
                android:layout_weight="1"
                android:background="#fff1f4f8"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/admissionTotalsValue"
                android:layout_weight="1"/>
        </TableRow>
        <!-- Total admiss. général -->
        <TableRow
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:textSize="18dp"
                android:text="@string/nutrition_grand_total_in_label"
                android:layout_span="3"
                android:background="#d3d3d3"/>
        </TableRow>
        <TableRow
            android:id="@+id/admissionGrandTableRow"
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/admissionGrandTotalsValue"
                android:layout_weight="1"/>
        </TableRow>
        <!-- Out -->
        <TableRow
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:textSize="18dp"
                android:text="@string/nutrition_out"
                android:layout_span="3"
                android:background="#d3d3d3" />
        </TableRow>
        <TableRow
            android:id="@+id/outTableRow"
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/outTotalValueM"
                android:layout_weight="1"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/outTotalValueF"
                android:layout_weight="1"
                android:background="#fff1f4f8"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/outTotalsValue"
                android:layout_weight="1"/>
        </TableRow>
        <!-- Total sorties générales -->
        <TableRow
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:textSize="18dp"
                android:text="@string/nutrition_grand_total_out_label"
                android:layout_span="3"
                android:background="#d3d3d3"/>

        </TableRow>
        <TableRow
            android:id="@+id/outGrandTableRow"
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/outGrandTotalsValue"
                android:layout_weight="1"/>
        </TableRow>
        <!-- Total end month -->
        <TableRow
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:textSize="18dp"
                android:text="@string/nutrition_total_end"
                android:layout_span="3"
                android:background="#d3d3d3"/>
        </TableRow>
        <TableRow
            android:id="@+id/endTableRow"
            style="@style/summarytableRow">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/endTotalValueM"
                android:layout_weight="1"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/endTotalValueF"
                android:layout_weight="1"
                android:background="#fff1f4f8"/>
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/endTotalsValue"
                android:layout_weight="1"/>
        </TableRow>
    </TableLayout>
</ScrollView>