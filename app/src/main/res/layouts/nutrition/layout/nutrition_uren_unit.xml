<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ui="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/totalStartLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_total_start"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/scrollView1"
            android:orientation="horizontal"
            android:weightSum="1">
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalStarMField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_m"
                android:singleLine="false"
                android:editable="true"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp" />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalStarFField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_f"
                android:singleLine="false"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp" />
        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/admissionLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_admission"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:id="@+id/newCasesField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_new_cases"
            android:singleLine="false"
            android:focusable="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/returnedField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_returned"
            android:singleLine="false"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/totalAdmLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_total_adm"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/scrollView2"
            android:orientation="horizontal"
            android:weightSum="1">
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalAdmMField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_m"
                android:singleLine="false"
                android:editable="true"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp" />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalAdmFField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_f"
                android:singleLine="false"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp" />
        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/totalTransferredLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_transferred"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/transferredField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_transferred"
            android:singleLine="false"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/OutsLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_out"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/healedField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_healed"
            android:singleLine="false"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/deceasedField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_deceased"
            android:singleLine="false"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/abandonField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_abandon"
            android:singleLine="false"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/respondingField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_not_responding"
            android:singleLine="false"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/totalOutsLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_total_out"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/scrollView3"
            android:orientation="horizontal"
            android:weightSum="1">
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalOutsMField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_m"
                android:singleLine="false"
                android:editable="true"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp" />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalOutsFField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_f"
                android:singleLine="false"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutReferred"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/referredLabel"
                android:linksClickable="false"
                android:text="@string/nutrition_referred"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_margin="10dp" />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/referredField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint=""
                android:singleLine="false"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
        </LinearLayout>
<!-- 
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/allOutsLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_all_out"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/allOutsField"
            android:imeOptions="actionNext"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="@string/nutrition_all_out"
            android:singleLine="false"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp" />
 -->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/totalEndLabel"
            android:linksClickable="false"
            android:text="@string/nutrition_total_end"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_margin="10dp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/scrollView4"
            android:orientation="horizontal"
            android:weightSum="1">
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalEndMField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_m"
                android:singleLine="false"
                android:editable="true"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp" />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/totalEndFField"
                android:imeOptions="actionNext"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="@string/nutrition_f"
                android:singleLine="false"
                android:layout_weight="0.50"
                android:layout_marginRight="10dp" />
        </LinearLayout>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/nutrition_monthly_save"
            android:id="@+id/saveButton"
            android:layout_margin="10dp" />
    </LinearLayout>
</ScrollView>