<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:shrinkColumns="*"
        android:stretchColumns="*"
        android:background="#ffffff">
        <!--URENAM + URENAS -->
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="center_horizontal">
            <TextView
                android:id="@+id/titleLabelUREN"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:text="URENAM + URENAS"
                android:layout_span="3"
                android:padding="10dp"
                android:background="#b0b0b0"
                android:textColor="#000"
                android:gravity="center" />
        </TableRow>
        <TableRow
            android:id="@+id/sexRow2"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <TextView
                android:id="@+id/totalsText"
                android:text="Total"
                android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/totalMText"
                android:text="@string/nutrition_m"
                android:layout_weight="1"
                android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/totalFText"
                android:text="@string/nutrition_f"
                android:layout_weight="1"
                android:background="#cac9c9"
                android:textColor="#000000"
                android:gravity="center"/>
        </TableRow>
        <!-- Total start month -->
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:text="@string/nutrition_total_start"
                android:layout_span="3"
                android:background="#b0b0b0"
                android:textColor="#000"
                android:gravity="center" />

        </TableRow>
        <TableRow
            android:id="@+id/tableRow1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <TextView
                android:id="@+id/startTotalsValue"
                android:layout_weight="1"
                android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/startTotalValueM"
                android:layout_weight="1"
                android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/startTotalValueF"
                android:layout_weight="1"
                android:background="#cac9c9"
                android:textColor="#000000"
                android:gravity="center"/>
        </TableRow>
        <!-- Admmission -->
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:text="@string/nutrition_admission"
                android:layout_span="3"
                android:background="#b0b0b0"
                android:textColor="#000"
                android:gravity="center" />

        </TableRow>
        <TableRow
            android:id="@+id/admissionTableRow"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <TextView
                android:id="@+id/admissionTotalsValue"
                android:layout_weight="1"
                android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/admissionTotalValueM"
                android:layout_weight="1"
                android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/admissionTotalValueF"
                android:layout_weight="1"
                android:background="#cac9c9"
                android:textColor="#000000"
                android:gravity="center"/>
        </TableRow>
        <!-- Out -->
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:text="@string/nutrition_out"
                android:layout_span="3"
                android:background="#b0b0b0"
                android:textColor="#000"
                android:gravity="center" />

        </TableRow>
        <TableRow
            android:id="@+id/outTableRow"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <TextView
                android:id="@+id/outTotalsValue"
                android:layout_weight="1"
                android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/outTotalValueM"
                android:layout_weight="1"
                android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/outTotalValueF"
                android:layout_weight="1"
                android:background="#cac9c9"
                android:textColor="#000000"
                android:gravity="center"/>
        </TableRow>
        <!-- Total end month -->
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:text="@string/nutrition_total_end"
                android:layout_span="3"
                android:background="#b0b0b0"
                android:textColor="#000"
                android:gravity="center" />

        </TableRow>
        <TableRow
            android:id="@+id/endTableRow"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <TextView
                android:id="@+id/endTotalsValue"
                android:layout_weight="1"
                android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/endTotalValueM"
                android:layout_weight="1"
                android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/endTotalValueF"
                android:layout_weight="1"
                android:background="#cac9c9"
                android:textColor="#000000"
                android:gravity="center"/>
        </TableRow>

        
    </TableLayout>
</ScrollView>