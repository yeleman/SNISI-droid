<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <!--URENAM + URENAS -->     
        <TableLayout
            style="@style/summaryTableLayout"
            android:id="@+id/tableURENAMAndURENAS">
        </TableLayout>
        <!--URENI -->
        <TableLayout
            style="@style/summaryTableLayout"
            android:id="@+id/tableURENI">
        </TableLayout>
        <!-- Intrants -->
        <TableLayout
            style="@style/summaryTableLayout"
            android:id="@+id/tableInputs">
            <TextView
                style="@style/summaryTableLabel"
                android:id="@+id/titleLabelInputs"
                android:textSize="18dp"
                android:text="Stocks"
                android:layout_span="3"
                android:padding="5dp"
                android:background="#b0b0b0" />
            <TableRow
                style="@style/summarytableRow"
                android:weightSum="1"
                android:background="#d3d3d3"
                android:baselineAligned="false">
                <TextView
                    android:text="Intrant"
                    android:gravity="left"
                    android:layout_weight="0.50"
                    android:paddingLeft="10dp"
                    android:textStyle="bold"
                    android:background="#fff8fbff" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:text="Solde"
                    android:layout_weight="0.50"
                    android:background="#fff8fbff"
                    android:singleLine="false"
                    android:textStyle="bold" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/plumpyRow"
                android:weightSum="1"
                android:background="#d3d3d3">
                <TextView
                    android:id="@+id/plumpyLabel"
                    android:text="@string/nutrition_plumpy_nut"
                    android:gravity="left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/plumpyBalance"
                    android:layout_weight="0.50"
                    android:background="#d3d3d3"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/milkF75Row"
                android:weightSum="1">
                <TextView
                    android:id="@+id/milkF75Label"
                    android:text="@string/nutrition_milk_f75"
                    android:gravity="left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/milkF75Balance"
                    android:layout_weight="0.50"
                    android:background="#fff8fbff"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/milkF100Row"
                android:weightSum="1">
                <TextView
                    android:id="@+id/milkF100Label"
                    android:text="@string/nutrition_milk_f100"
                    android:background="#dcdcdc"
                    android:gravity="left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/milkF100Balance"
                    android:background="#d3d3d3"
                    android:layout_weight="0.50"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/resomalRow"
                android:weightSum="1">
                <TextView
                    android:id="@+id/resumeLabel"
                    android:text="@string/nutrition_resomal"
                    android:gravity="left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/resumeBalance"
                    android:layout_weight="0.50"
                    android:background="#fff8fbff"
                    android:textSize="17dp" />
            </TableRow>

            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/plumpySupRow"
                android:weightSum="1">
                <TextView
                    android:id="@+id/plumpySupLabel"
                    android:text="@string/nutrition_plumpy_sup"
                    android:background="#dcdcdc"
                    android:gravity="left"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/plumpySupBalance"
                    android:background="#d3d3d3"
                    android:layout_weight="0.50"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/supercerealRow"
                android:weightSum="1">
                <TextView
                    android:id="@+id/supercerealLabel"
                    android:text="@string/nutrition_supercereal"
                    android:gravity="left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/supercerealBalance"
                    android:layout_weight="0.50"
                    android:background="#fff8fbff"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/supercerealPlusRow"
                android:weightSum="1">
                <TextView
                    android:id="@+id/supercerealPlusLabel"
                    android:text="@string/nutrition_supercereal_plus"
                    android:background="#dcdcdc"
                    android:gravity="left"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/supercerealPlusBalance"
                    android:background="#d3d3d3"
                    android:layout_weight="0.50"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/oilRow"
                android:weightSum="1">
                <TextView
                    android:id="@+id/oilLabel"
                    android:text="@string/nutrition_oil"
                    android:gravity="left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/oilBalance"
                    android:layout_weight="0.50"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/amoxycilline125mgVialsRow"
                android:weightSum="1"
                android:background="#dcdcdc">
                <TextView
                    android:id="@+id/amoxycilline125mgVialsLabel"
                    android:text="@string/nutrition_amoxycilline_125mg_vials"
                    android:gravity="left"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/amoxycilline125mgVialsBalance"
                    android:layout_weight="0.50"
                    android:background="#d3d3d3"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/amoxycilline250mgVialsRow"
                android:weightSum="1">
                <TextView
                    android:id="@+id/amoxycilline250mgVialsLabel"
                    android:text="@string/nutrition_amoxycilline_250mg_caps"
                    android:gravity="left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/amoxycilline250mgVialsBalance"
                    android:layout_weight="0.50"
                    android:background="#fff8fbff"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/albendazole400mgRow"
                android:weightSum="1"
                android:background="#dcdcdc">
                <TextView
                    android:id="@+id/albendazole400mgLabel"
                    android:text="@string/nutrition_albendazole_400mg"
                    android:gravity="left"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/albendazole400mgBalance"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:background="#d3d3d3"/>
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/vita100KUiInjectableRow"
                android:weightSum="1">
                <TextView
                    android:id="@+id/vita100KUiInjectableLabel"
                    android:text="@string/nutrition_vita_100K_ui_injectable"
                    android:gravity="left"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/vita100KUiInjectableBalance"
                    android:layout_weight="0.50"
                    android:background="#fff8fbff"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/vita200KUiInjectableRow"
                android:weightSum="1"
                android:background="#dcdcdc">
                <TextView
                    android:id="@+id/vita200KUiInjectableLabel"
                    android:text="@string/nutrition_vita_200K_ui_injectable"
                    android:gravity="left"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/vita200KUiInjectableBalance"
                    android:layout_weight="0.50"
                    android:background="#d3d3d3"
                    android:textSize="17dp" />
            </TableRow>
            <TableRow
                style="@style/summarytableRow"
                android:id="@+id/ironFolicAcidRow"
                android:weightSum="1"
                android:layout_marginBottom="10dp">
                <TextView
                    android:id="@+id/ironFolicAcidLabel"
                    android:text="@string/nutrition_vita_200K_ui_injectable"
                    android:layout_weight="0.50"
                    android:textSize="17dp"
                    android:paddingLeft="10dp"
                    android:gravity="left" />
                <TextView
                    style="@style/summaryTableLabel"
                    android:id="@+id/ironFolicAcidBalance"
                    android:layout_weight="0.50"
                    android:background="#fff8fbff"
                    android:textIsSelectable="false"
                    android:textSize="17dp" />
            </TableRow>
        </TableLayout>
    </LinearLayout>
</ScrollView>