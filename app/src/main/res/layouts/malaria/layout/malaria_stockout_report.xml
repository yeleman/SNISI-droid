<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_act_children"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:id="@+id/actChildrenLabel"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupChildren"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="OUI"
                    android:id="@+id/actChildrenYesField"
                    android:checked="false"
                    android:layout_weight="0.50" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/actChildrenNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />

            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/actYouthLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_act_youth"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupYouth"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/actYouthYesField"
                    android:layout_weight="0.50" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/actYouthNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/adultLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_act_adult"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupAdult"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/adultYesField"
                    android:layout_weight="0.50" />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/adultNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50"  />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/artemetherLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_artemether"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupArtemether"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/artemetherYesField"
                    android:layout_weight="0.50"  />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/artemetherNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/quinineLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_quinine"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupQuinine"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/quinineYesField"
                    android:layout_weight="0.50" />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/quinineNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/serumLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_serum"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupSerum"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/serumYesField"
                    android:layout_weight="0.50" />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/serumNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50"  />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/bednetLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_bednet"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupBednet"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/bednetYesField"
                    android:layout_weight="0.50"  />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/bednetNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/rdtLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_rdt"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupRdt"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/rdtYesField"
                    android:layout_weight="0.50"  />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/rdtNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50"  />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/spLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_stockout_sp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupSp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/spYesField"
                    android:layout_weight="0.50" />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/spNoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/anc1Label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_total_anc_1"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupAnc1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/anc1YesField"
                    android:layout_weight="0.50"  />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/anc1NoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/sp1Label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_total_sp_1"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupSp1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/sp1YesField"
                    android:layout_weight="0.50"  />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/sp1NoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50"  />
            </RadioGroup>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/sp2Label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/malaria_total_sp_2"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
            <RadioGroup
                android:id="@+id/radioGroupSp2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="15dp">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Oui"
                    android:id="@+id/sp2YesField"
                    android:layout_weight="0.50" />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Non"
                    android:id="@+id/sp2NoField"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.50" />
            </RadioGroup>
        </LinearLayout>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/malaria_monthly_save"
            android:id="@+id/saveButton"
            android:layout_margin="10dp" />

    </LinearLayout>
</ScrollView>